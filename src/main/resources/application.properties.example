eureka:
    client:
        serviceUrl:
            defaultZone: ${EUREKA_URI:http://XXXXXXXXXXX:11111111111/XXXXXXXXXXXXXX}
    instance:
        preferIpAddress: false
fountation:
    service:
        account:
            url: lb://fountation-account-service
logging:
    level:
        org:
            springframework:
                security:
                    oauth2:
                        server:
                            resource:
                                authentication: DEBUG
        reactor:
            core:
                publisher: DEBUG
            netty: DEBUG
server:
    port: 0
spring:
    application:
        name: fountation-user-service
    main:
        web-application-type: reactive
    r2dbc:
        password: 11111111111
        url: r2dbc:postgresql://XXXXXXXXXXX:11111111111/XXXXXXXXXXX
        username: postgres
    security:
        oauth2:
            client:
                provider:
                    auth0:
                        authorization-uri: https://XXXXXXXXXXX/authorize?audience=https://XXXXXXXXXXX/api/v2/
                        issuer-uri: https://XXXXXXXXXXX/
                        jwk-set-uri: https://XXXXXXXXXXX/.well-known/jwks.json
                        token-uri: https://XXXXXXXXXXX/oauth/token
                        user-info-uri: https://XXXXXXXXXXX/userinfo
                registration:
                    auth0:
                        authorization-grant-type: authorization_code
                        client-id: XXXXXXXXXXX
                        client-name: Auth0
                        client-secret: XXXXXXXXXXX
                        provider: auth0
                        redirect-uri: http://XXXXXXXXXXXXXXXXXXXXXX
                        scope: openid,profile,email
                    internal-service:
                        authorization-grant-type: client_credentials
                        client-id: XXXXXXXXXXX
                        client-name: Auth0
                        client-secret: XXXXXXXXXXX
                        provider: auth0
                        scope: internal:service
            resourceserver:
                jwt:
                    audiences:
                        - https://XXXXXXXXXXX
                        - https://XXXXXXXXXXX
                    issuer-uri: https://XXXXXXXXXXX
                    jwk-set-uri: https://XXXXXXXXXXX
