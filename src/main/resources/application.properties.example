server:
    port: 0
spring:
    application:
        name: fountation-user-service
    main:
        web-application-type: reactive
    security:
        oauth2:
            resourceserver:
                jwt:
                    audiences:
                        - https://dev-XXXXXXXXXXXXXX.us.auth0.com/api/v2/
                    issuer-uri: https://dev-XXXXXXXXXXX.us.auth0.com/
                    jwk-set-uri: https://dev-XXXXXXXXXXXXXX.us.auth0.com/.well-known/jwks.json
eureka:
    client:
        serviceUrl:
            defaultZone: ${EUREKA_URI:http://localhost:8761/eureka}
    instance:
        preferIpAddress: true
logging:
    level:
        org:
            springframework:
                security:
                    oauth2:
                        server:
                            resource:
                                authentication: DEBUG
                        jwt: DEBUG
        com:
            nimbusds:
                jose: DEBUG